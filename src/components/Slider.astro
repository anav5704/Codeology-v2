<style is:inline>

#sliderholder:before, #sliderholder:after  {
height: 100%;
width: 25px;
content: "";
position: absolute;
z-index: 2;
background: linear-gradient(to right, var(--CCool) 0%, rgba(0,0,0,0) 100%);
}

.dark #sliderholder:before ,.dark #sliderholder:after  {
background: linear-gradient(to right, var(--CBlack) 0%, rgba(0,0,0,0) 100%);

}

#sliderholder:before {
top: 0;
left: 0;
}

#sliderholder:after  {
top: 0;
right: 0;
rotate: 180deg;
}

#slider {
    width: calc(180px * 30);
}

.slider {
    height: 100px;
    width: calc(180px * 15);
    animation: scroll 50s linear infinite;
}

.slider svg {
    aspect-ratio: 1/1;
    height: 100%;
    margin: 0px 50px;
}

@media only screen and (max-width: 600px) {
    .slider svg {
    aspect-ratio: 1/1;
    height: 75%;
    margin: 0px 25px;
}
}

@keyframes scroll {
    0% {
        transform: translateX(0);
    }
    100% {
        transform: translateX(-100%);
    }
}
</style>