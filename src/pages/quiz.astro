<style is:inline>
    
     input[type = "checkbox"]:checked ~ #content {
        max-height: 200px;
        opacity: 1;
    }

     input[type = "checkbox"]:checked ~ h3 label svg {
        rotate: -180deg;
    }
    
    #content {
        transition: all .3s ease 0s;
        opacity: 0;
    }

    h3 label svg {
        transition: all .3s ease;
    }

    .tech svg {
        aspect-ratio: 1/1;
        height: 50px;
        margin-right: 15px;
    }


    .blur-load {
